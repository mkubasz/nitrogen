@using RejestrFaktur.Models
@using RejestrFaktur.utils
@model ObiektDoWidoku<JednostkaMiary>
@{
    Layout = "~/Views/Wspolne/szablonWartosciSlownikowe.cshtml";
}

<h1>Czy chcesz usunąć poniższy wpis z bazy danych?</h1>


@Html.Partial("~/Views/JednostkiMiary/SzczegolyForm.cshtml", Model.Edytowany)

@using (Html.BeginForm("Usun", "JednostkiMiary", FormMethod.Post))
    {

        <div>
            @Html.HiddenFor(model=> model.Edytowany.Id)
            @Html.HiddenFor(model=> model.Edytowany.NazwaJednostki)
            @Html.HiddenFor(model=> model.Edytowany.SymbolJednostki)
            <input type="submit" value="Usuń" />
        </div>
    }

@Html.ActionLink("Powrót do listy obiektów", "Index")

