<section class="contact">
    <div class="container">
        <div class="row">
            <div class="contact-caption clearfix">
                <div class="contact-heading text-center">
                    <h2>Kontakt</h2>
                </div>

                <div class="col-md-5 contact-info text-left">
                    <h3>Infromacje kontaktowe:</h3>
                    <div class="info-detail">
                        <ul><li><i class="fa fa-map-marker"></i><span>Adres:</span> Płock</li></ul>
                        <ul><li><i class="fa fa-phone"></i><span>Telefon:</span> 692 792 967</li></ul>
                        <ul><li><i class="fa fa-envelope"></i><span>Email:</span> marcin.mazurowski@o2.pl</li></ul>
                    </div>
                </div>


                <div class="col-md-6 col-md-offset-1 contact-form">
                    <h3>Wyślij nam wiadomość</h3>

                    <form class="form">
                        <input class="name" type="text" placeholder="Imię i Nazwisko">
                        <input class="email" type="email" placeholder="Email">
                        <input class="phone" type="text" placeholder="Nr telefonu">
                        <textarea class="message" name="message" id="message" cols="30" rows="10" placeholder="Wiadomość"></textarea>
                        <input class="submit-btn" type="submit" value="WYŚLIJ">
                    </form>
                </div>

            </div>
        </div>
    </div>
</section><!-- end of contact section -->
